/**
 * Definition for a binary tree node.
 * public class TreeNode {
 *     int val;
 *     TreeNode left;
 *     TreeNode right;
 *     TreeNode() {}
 *     TreeNode(int val) { this.val = val; }
 *     TreeNode(int val, TreeNode left, TreeNode right) {
 *         this.val = val;
 *         this.left = left;
 *         this.right = right;
 *     }
 * }
 */
class Solution {
    public boolean isSymmetric(TreeNode root) {
        if(root == null)
            return false;
        return isSimilar(root.left, root.right);
        
    }
    public boolean isSimilar(TreeNode X, TreeNode Y){
        if(X == null && Y == null)
            return X == Y;
      
        
     if(X!= null && Y!= null)   
        if(X.val != Y.val) 
            return false;
        
        return (X!= null && Y!= null) && isSimilar(X.left, Y.right) && isSimilar(X.right, Y.left);  
    }
}
